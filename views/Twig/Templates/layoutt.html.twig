<!doctype html>
<html>
    <head>      
        <meta charset="utf-8">
        <title>{{ titulo }}</title>

        {% block stylesheets %}
        <link rel="stylesheet" href="./css/style.css">

        {% endblock %}
    </head>     

    <body>
        <header>
            <div id="logo">
                <img src="./imagen/logocolores.jpg" alt="logo">
            </div>
            
        
        </header>


        
        {% block menu %}{% endblock %}
            

        <main>
            <div class="contenido">
                    <h1>{{ titulo }}</h1>
                    {% block contenido %}{% endblock %}
            </div>
        </main>
        

        <br class="clear" />    
        <footer> 
            <p>Proyecto de Software 2014 - Grupo 76 © 2014 Banco Alimentario de La Plata</p>
        </footer>

        
    </body>
</html> 
    
                    
